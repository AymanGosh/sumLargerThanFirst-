package main

import "fmt"

func main() {

	var arr=[]int {6,2,8,11,3,6}
	fmt.Println(sumLargerThanFirst(arr))
	
	
}
func sumLargerThanFirst(arr []int)int {
	
	
	var sum int
	for i := 1; i < len(arr); i++{
     		 if(arr[i]>arr[0]){
			sum=sum+arr[i]
		}
		
    	}
	return sum
}
